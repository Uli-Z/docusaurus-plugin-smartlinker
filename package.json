{
  "name": "docusaurus-plugin-smartlinker",
  "version": "0.0.0",
  "description": "Smartlinker plugin for Docusaurus with remark integration",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./theme": "./dist/theme/index.js",
    "./theme/styles.css": "./dist/theme/styles.css",
    "./theme/*": "./dist/theme/runtime/*",
    "./remark": {
      "types": "./dist/remark/index.d.ts",
      "import": "./dist/remark/index.js",
      "require": "./dist/remark/index.cjs"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18 <25"
  },
  "scripts": {
    "build": "node -e \"require('fs').rmSync('dist', {recursive: true, force: true});\" && tsc -p tsconfig.json && node ./scripts/postbuild-verify.mjs && node ./scripts/remark/build-cjs.js",
    "test": "vitest run",
    "lint": "echo \"@(optional) add eslint later\"",
    "site:dev": "pnpm --filter @examples/site dev",
    "site:build": "pnpm --filter @examples/site build",
    "site:serve": "pnpm --filter @examples/site serve",
    "reset": "rm -rf node_modules examples/site/node_modules dist && pnpm install && pnpm run build && pnpm --filter @examples/site build",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "@docusaurus/core": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "unified": "^11.0.0"
  },
  "dependencies": {
    "@docusaurus/mdx-loader": "^3.0.0",
    "@mdx-js/mdx": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@radix-ui/react-tooltip": "^1.2.8",
    "gray-matter": "^4.0.3",
    "react-markdown": "^9.0.3",
    "unist-util-visit": "^5.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@docusaurus/types": "^3.8.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/mdast": "^4.0.4",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/unist": "^3.0.2",
    "jsdom": "^24.1.0",
    "mdast-util-to-string": "^4.0.0",
    "remark-mdx": "^3.1.1",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "typescript": "^5.5.4",
    "unified": "^11.0.5",
    "vfile": "^6.0.3",
    "vitest": "^2.0.5"
  },
  "pnpm": {
    "overrides": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "estree-walker": "^3.0.3",
      "estree-util-build-jsx": "^3.0.1",
      "recma-build-jsx": "^1.0.0",
      "@mdx-js/mdx": "^3.1.1"
    }
  }
}
