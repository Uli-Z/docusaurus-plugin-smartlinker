{
  "name": "remark-smartlinker",
  "version": "0.2.1",
  "private": true,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "devDependencies": {
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.4",
    "@types/node": "^20.14.10",
    "@types/unist": "^3.0.2",
    "@types/mdast": "^4.0.4",
    "@types/vfile": "^4.0.0"
  }
}
