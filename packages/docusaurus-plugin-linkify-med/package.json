    "@testing-library/user-event": "^14.5.2",
    "jsdom": "^24.1.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "peerDependencies": {
    "@docusaurus/core": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.1.1",
    "@radix-ui/react-tooltip": "^1.2.8",
    "gray-matter": "^4.0.3",
    "zod": "^3.23.0"
  }
